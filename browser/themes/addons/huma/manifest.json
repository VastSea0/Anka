{
    "manifest_version": 2,
    "version": "1.0",
    "name": "H\u00fcma Tarayici Temasi",
    "description": "Hüma web tarayıcısı için varsayılan tarayıcı teması",
    "author": "Egehan",
    "theme": {
      "images": {
        "additional_backgrounds": [
          "0.svg",
